{
  "defaultSeverity": "error",
  "extends": ["tslint-xo/space"],
  "jsRules": {},
  "rules": {
    "no-reference": false,
    "semicolon": [true, "never"],
    "promise-function-async": false,
    "object-curly-spacing": [true, "always"],
    "type-literal-delimiter": false,
    "no-single-line-block-comment": false
  },
  "rulesDirectory": []
}
