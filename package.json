{
  "name": "majo",
  "version": "0.6.3",
  "description": "my badass project",
  "repository": {
    "url": "egoist/majo",
    "type": "git"
  },
  "main": "dist/majo.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "prepublishOnly": "npm run build",
    "lint": "xo",
    "build": "bili --inline --external fast-glob"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/examples/"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "bili": "^3.3.0",
    "buble": "^0.15.2",
    "eslint-config-rem": "^3.0.0",
    "jest-cli": "^22.4.3",
    "mkdirp": "^0.5.1",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem",
    "envs": [
      "jest"
    ],
    "ignores": [
      "test/fixture/**",
      "test/output/**",
      "examples/**"
    ],
    "rules": {
      "guard-for-in": 0
    }
  },
  "dependencies": {
    "fast-glob": "^2.2.3"
  }
}
